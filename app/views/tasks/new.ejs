<div class="page-header">
  <h1>New task</h1>
</div>

<% form_for(task, {action: path_to.tasks, method: 'POST', id: "task_form"}, function (form) { %>
  <%- partial('tasks/form.ejs', {locals: {form: form, task: task}}) %>
  <div class="actions">
    <%- form.submit('Create task', {class: 'btn primary'}) %> or
    <%- link_to('Cancel', path_to.tasks, {class: 'btn'}) %>
  </div>
<% });%>
